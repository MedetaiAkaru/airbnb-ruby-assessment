<div class="row"></div>
	<% @property.each do |eachproperty| %>
	<div class="row">
		<div class="col s1">
      <img src="<%= eachproperty.image %>"
		</div>
		<div class="col s11">
			<div class="card">
        <div class="card-content">
          <span class="card-title"><a href=/properties/<%= eachproperty.id%> ><%= eachproperty.title %></a></span>
          <p>Price: <%= eachproperty.price %></p>
          <p><%= eachproperty.description %></p>
        </div>
        <div class="card-action">
          
			<% if current_user.id == eachpost.user.id %>!! THIS IS YOUR PROPERTY !!<% end %>
        </div>
      </div>
		</div>
	</div>
<% end %>


<% if logged_in? %>
  <div class="fixed-action-btn horizontal" style="bottom: 45px; right: 24px;">
    <a class="btn-floating btn-large red" href="/properties/new">
      <i class="large material-icons">mode_edit</i>
    </a>
  </div>
<% end %>